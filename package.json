{
  "name": "CocosSymbol",
  "displayName": "Cocos Symbol Parser",
  "description": "parse symbols in cocos creator project",
  "version": "0.0.2",
  "publisher": "flyingkisser",
  "author": "flyingkisser",
  "engines": {
    "vscode": "^1.94.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand::cocos.parse",
    "onCommand::cocos.goto",
    "onLanguage:javascript",
    "onLanguage:typescript"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "cocos.parse",
        "title": "cocos parse symbole"
      },
      {
        "command": "cocos.goto",
        "title": "cocos goto def"
      },
      {
        "command": "cocos.findReferences",
        "title": "cocos find references"
      },
      {
        "command": "cocos.showSavedReferences",
        "title": "Show Saved References"
    }
    ],
    "keybindings": [
      {
        "command": "cocos.parse",
        "key": "ctrl+shift+1",
        "mac": "cmd+shift+1",
        "when": "editorTextFocus"
      },
      {
        "command": "cocos.goto",
        "key": "ctrl+d",
        "mac": "cmd+d",
        "when": "editorTextFocus"
      },
      {
        "command": "cocos.findReferences",
        "key": "ctrl+shift+2",
        "mac": "cmd+shift+2",
        "when": "editorTextFocus"
      },
      {
        "command": "cocos.showSavedReferences",
        "key": "ctrl+shift+9",
        "mac": "cmd+shift+9",
        "when": "editorTextFocus"
    }
      
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "_pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test",
    "build:asar": "asar pack ./node_modules ./dist/node_modules.asar"
  },
  "dependencies": {
    "@types/node": "^22.7.5",
    "@types/vscode": "^1.94.0",
    "typescript": "^5.6.3"
  },
  "devDependencies11": {
    "@types/mocha": "^10.0.9",
    "@types/node": "^22.7.5",
    "@types/vscode": "^1.94.0",
    "@typescript-eslint/eslint-plugin": "^8.7.0",
    "@typescript-eslint/parser": "^8.7.0",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1",
    "eslint": "^9.11.1",
    "typescript": "^5.6.3"
  },
  "devDependencies": {
    "@types/node": "^22.7.5",
    "@types/vscode": "^1.94.0",
    "asar": "^3.2.0",
    "typescript": "^5.6.3"
  }
}
